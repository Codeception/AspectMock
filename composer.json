{
    "name": "codeception/aspect-mock",
    "description": "Experimental Mocking Framework powered by Aspects",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Bodnarchuk",
            "email": "davert@codeception.com"
        }
    ],
    "autoload": {
           "psr-0": {
               "AspectMock": "src/"
           }
       },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "poster/goaop-framework",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/WalterWoshid/goaop-framework.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.4",
        "poster/goaop-framework": "1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/finder": "^4.4 | ^5.4 | ^6.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/verify": "^2.2",
        "codeception/specify": "^2.0",
        "consolidation/robo": "^3.0"
    }
}
